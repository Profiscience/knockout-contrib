{
  "name": "@profiscience/framework",
  "version": "1.0.0-alpha.2",
  "description": "An opinionated framework with first-class TypeScript support for building modern SPAs with KnockoutJS",
  "scripts": {
    "build:docs": "rm -r docs && typedoc --out docs --name @profiscience/framework --readme none --mode file --theme markdown --excludePrivate --excludeNotExported src/index.ts"
  },
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Profiscience/framework.git"
  },
  "keywords": [
    "knockoutjs",
    "ko",
    "knockout",
    "framework",
    "spa",
    "router",
    "data",
    "model",
    "single-page-app",
    "typescript"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/Profiscience/framework/issues"
  },
  "homepage": "https://github.com/Profiscience/framework#readme",
  "dependencies": {
    "@profiscience/knockout-contrib-router": "^1.0.0-alpha.7",
    "@profiscience/knockout-contrib-utils": "^1.0.0-alpha.5",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@profiscience/knockout-contrib-jest-matchers": "^1.0.0-alpha.0",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.11"
  },
  "peerDependencies": {
    "knockout": "^3.4.2",
    "lodash": "^4.17.4"
  }
}
