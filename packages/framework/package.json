{
  "name": "@profiscience/framework",
  "private": false,
  "version": "0.0.0",
  "description": "An opinionated framework with first-class TypeScript support for building modern SPAs with KnockoutJS",
  "scripts": {
    "build": "rm -r dist && tsc",
    "test": "jest",
    "build:docs": "rm -r docs && typedoc --out docs --name @profiscience/framework --mode file --theme markdown --excludePrivate --excludeNotExported src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Profiscience/framework.git"
  },
  "keywords": [
    "knockoutjs",
    "ko",
    "knockout",
    "framework",
    "spa",
    "router",
    "data",
    "model",
    "single-page-app",
    "typescript"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/Profiscience/framework/issues"
  },
  "homepage": "https://github.com/Profiscience/framework#readme",
  "dependencies": {
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.11",
    "typescript": "^2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": "__tests__/[^_].*\\.ts$",
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
