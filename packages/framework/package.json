{
  "name": "@profiscience/knockout-contrib-framework",
  "version": "1.0.0-alpha.2",
  "description": "An opinionated framework with first-class TypeScript support for building modern SPAs with KnockoutJS",
  "scripts": {
    "build": "../../node_modules/.bin/taskr build -d ../..",
    "test": "DIR=$PWD; cd ../..; yarn test $DIR",
    "build:docs": "typedoc --out ../../dist/api/framework --name @profiscience/knockout-contrib-framework --readme none --mode file --theme markdown --excludePrivate --excludeNotExported src/index.ts"
  },
  "main": "dist/knockout-contrib-framework.js",
  "global": "ko.contrib.framework",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Profiscience/knockout-contrib.git"
  },
  "keywords": [
    "knockoutjs",
    "ko",
    "knockout",
    "framework",
    "spa",
    "router",
    "data",
    "model",
    "single-page-app",
    "typescript"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/Profiscience/knockout-contrib/issues"
  },
  "homepage": "https://github.com/Profiscience/knockout-contrib#readme",
  "dependencies": {
    "@profiscience/knockout-contrib-router": "1.0.0",
    "@profiscience/knockout-contrib-utils": "1.0.0-alpha.6",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@profiscience/knockout-contrib-jest-matchers": "1.0.0-alpha.1"
  },
  "peerDependencies": {
    "knockout": "^3.4.0",
    "lodash": "^4.17.0"
  }
}
