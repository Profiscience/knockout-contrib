{
  "name": "@profiscience/knockout-contrib-framework",
  "version": "1.0.0-alpha.3",
  "description": "An opinionated framework with first-class TypeScript support for building modern SPAs with KnockoutJS",
  "scripts": {
    "build": "../../node_modules/.bin/taskr build -d ../..",
    "test": "DIR=$PWD; cd ../..; yarn test $DIR",
    "build:docs": "typedoc --out docs/typedoc --name framework --readme none --mode file --theme markdown --excludePrivate --excludeNotExported index.ts"
  },
  "main": "dist/knockout-contrib-framework.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Profiscience/knockout-contrib.git"
  },
  "keywords": [
    "knockoutjs",
    "ko",
    "knockout",
    "framework",
    "spa",
    "router",
    "data",
    "model",
    "single-page-app",
    "typescript"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": "https://github.com/Profiscience/knockout-contrib/issues",
  "homepage": "https://github.com/Profiscience/knockout-contrib#readme",
  "dependencies": {
    "tslib": "^1.8.0",
    "@profiscience/knockout-contrib-framework-model-builders-base": "^1.0.0-alpha.1",
    "@profiscience/knockout-contrib-framework-model-builders-data": "^1.0.0-alpha.1",
    "@profiscience/knockout-contrib-framework-model-builders-view": "^1.0.0-alpha.1",
    "@profiscience/knockout-contrib-framework-model-mixins-collection": "^1.0.0-alpha.1",
    "@profiscience/knockout-contrib-framework-model-mixins-subscription-disposal": "^1.0.0-alpha.1",
    "@profiscience/knockout-contrib-framework-plugin": "^1.0.0-alpha.1"
  },
  "peerDependencies": {
    "knockout": "^3.4.0"
  }
}
