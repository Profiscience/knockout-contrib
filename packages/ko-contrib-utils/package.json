  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/Profiscience/ko-contrib-utils/issues"
  },
  "homepage": "https://github.com/Profiscience/ko-contrib-utils#readme",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.16.1",
    "eslint-config-profiscience": "^1.3.1",
    "fly": "^2.0.5",
    "fly-babel": "^2.1.1",
    "fly-concat": "^1.2.0",
    "fly-rollup": "^2.0.0",
    "knockout": "^3.4.0",
    "nyc": "^10.0.0",
    "rollup-plugin-babel": "^2.7.1"
