{
  "name": "ko-contrib-utils",
  "version": "0.2.1",
  "description": "Handy Utilities for Knockout JS",
  "main": "dist/ko-contrib-utils.js",
  "module": "dist/modules/index.js",
  "scripts": {
    "test": "export NODE_ENV=test; nyc --reporter=lcov --reporter=html ava --verbose --serial",
    "test-watch": "ava --watch",
