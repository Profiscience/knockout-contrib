  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Profiscience/knockout-contrib.git"
  },
  "keywords": [
    "knockoutjs",
    "ko",
    "knockout",
    "spa",
    "router",
    "data",
    "model",
    "single-page-app",
    "typescript"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": "https://github.com/Profiscience/knockout-contrib/issues",
  "homepage": "https://github.com/Profiscience/knockout-contrib#readme",
  "dependencies": {
    "@profiscience/knockout-contrib-model-builders-base": "^1.0.0-alpha.3",
    "@profiscience/knockout-contrib-model-builders-data": "^1.0.0-alpha.8",
