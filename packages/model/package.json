  "description": "An opinionated model with first-class TypeScript support and router integration with KnockoutJS",
  "scripts": {
    "generate": "cd tasks && ts-node ./generate.ts",
    "build": "yarn generate && ../../node_modules/.bin/taskr build -d ../..",
    "test": "DIR=$PWD; cd ../..; yarn test $DIR"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
