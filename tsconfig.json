{
  "compilerOptions": {
    "target": "es2017",
    "importHelpers": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "h",
    "strict": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "baseUrl": "./",
    "lib": ["dom", "es5", "es2015", "esnext.asynciterable"],
    "paths": {
      "@profiscience/knockout-contrib-router": ["./packages/router/src"]
    }
  },
  "include": ["**/*"],
  "exclude": ["**/dist", "**/examples", "templates"]
}
