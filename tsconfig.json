{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "module": "commonjs",
    "importHelpers": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "dom",
      "es5",
      "es2015",
      "esnext.asynciterable"
    ],
    "typeRoots": [
      "@types",
      "node_modules/@types"
    ]
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "templates",
    "dist",
    "tasks"
  ]
}